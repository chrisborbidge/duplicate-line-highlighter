{
  "name": "duplicate-line-highligher",
  "displayName": "Duplicate Line highligher",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:javascript",
    "onLanguage:plaintext"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "properties": {
        "duplicateLineHighlightColor": {
          "type": "string",
          "default": "rgba(255, 0, 0, 0.5)",
          "description": "The color used to highlight duplicate lines in the editor."
        }
      }
    },
    "keybindings": [
      {
        "command": "duplicateLineHighlighter.decorateDuplicateLines",
        "key": "u",
        "when": "editorTextFocus && !editorReadonly",
        "mac": "cmd+u"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "string-similarity": "^4.0.4"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
